<h1>Create a new location</h1>
<div class="row">
  <div class="col-md-8 col-md-offset-2 jumbotron">
    <%= form_for @location do |f| %>

    <%= f.label :Name %>
    <%= f.text_field :name, class: 'form-control' %>

    <%= f.submit "Create new location", class: "btn btn-success" %>
<% end %>
  </div>
</div>